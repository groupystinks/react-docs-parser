{
  "name": "react-docs-parser",
  "version": "0.0.1",
  "description": "parse react components into documentation-ready AST",
  "main": "src/index.js",
  "bin": {
    "react-docs": "bin/react-docs"
  },
  "scripts": {
    "watch": "babel src/ --out-dir dist/ --watch",
    "build": "rm -rf dist/ && babel src/ --out-dir dist/ --ignore __tests__,__mocks__",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Lai Chia-Sheng <jason0911520@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "5.8.25",
    "commander": "2.9.0",
    "react-docgen": "2.6.1"
  },
  "devDependencies": {
    "babel": "5.6.23",
    "eslint": "1.10.3",
    "eslint-loader": "1.1.1",
    "eslint-plugin-react": "3.11.3"
  }
}
