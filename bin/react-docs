#!/usr/bin/env node

var program = require('commander')
var pjson = require('../package.json')
var reactDocsParser = require('../src');

program
    .version(pjson.version)
    .option('-p, --path <path>', 'path of react components')
    .parse(process.argv)

if (program.path) console.log('%s right here', program.path)
